<div class="row">
  <div class="large-12 columns">
    <h3>Trash</h3>

    <table>
      <tr>
        <th>Type</th>
        <th>Title</th>
        <th>Version Description</th>
        <th></th>
      </tr>
      <% @versions.each do |v| %>
          <% reified_version = v.reify %>
          <tr>
            <td><%= v.item_type %></td>
            <td><%= reified_version.title %></td>
            <td><%= reified_version.version_description %></td>
            <td><%= link_to 'View', version_path(v) %></td>
          </tr>
      <% end %>
    </table>
  </div>
</div>